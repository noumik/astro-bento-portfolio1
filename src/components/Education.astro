---
import Card from "./Card/index.astro";
import img from '../../src/lib/school.png';

const image = [
  { img: img },
]

---

<Card colSpan="md:col-span-1" rowSpan="md:row-span-3" title="Education" img={image[0].img.src}, opac="0.3", 
cubic="transform 20s cubic-bezier(0,1.95,.6,.96)", orig="center left", 
transformation="scale(1.5) rotate(-30deg) translateX(-30%) translateY(10%)", 
scl="scale(1.5) rotate(-30deg)", backpos="center right 125%">
  <div class="flex flex-col gap-2">
    <div class="text-sm font-light">
      <p>
        <br />
        Georgia Institute of Technology<br />
        M.S., Data Science<br />
        July 2025 (e)
        <br /><br />
        The University of Texas at Austin<br />
        B.B.A.,<br />
        Management Information Systems<br />
        May 2020
        <br />
      </p>
  </div>
</Card>
